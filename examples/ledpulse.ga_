\ require delay
\ require gpio
\ demo Led Pulse

node 500
: ledpulse
   
    0x30000 !b . .   \ led on
    pause
   
    0x20000 !b . .   \ led off
    pause
    5ms
    ledpulse

    
: pause
	5000 for 10000 
	for next . . next ;
: 5ms ( 5ms )
    5 for 999 (  time )
	for 416 for unext next  next ;
